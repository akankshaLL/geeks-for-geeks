class Solution:
    #Function to return list containing first n fibonacci numbers.
    def fibonacciNumbers(self,n):
        fib_series=[]
        
        if n>=1:
            fib_series.append(0)
            
        if n>=2:
            fib_series.append(1)
            
        for i in range(2,n):
            next_fib= fib_series[i-1]+fib_series[i-2]
            fib_series.append(next_fib)
            
        return fib_series
